class Solution:
    def fib(self, n: int) -> int:
        a=0
        b=1
        if n==0:
            return 0
        if n==1:
            return 1
        else: 
            n=n-1
            c=0
            
            while(n):
                c=a+b
            
                a=b
                b=c
                n-=1
        
            return c
